{
  "name": "fast-hot-keys",
  "private":false,
  "version": "0.1.3",
  "description": "Un hook personalizado para capturar una combinación de teclas y ejecutar una función especificada",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
    "exports":{
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
  
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Anubix47/faster-hot-keys.git",
    "directory": "packages/fast-hot-keys"
  },
  "keywords": [
    "react",
    "hook",
    "typescript", 
    "hotkey"
  ],
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }

}
